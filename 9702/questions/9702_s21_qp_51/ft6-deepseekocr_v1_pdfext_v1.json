            {
                "type": "text",
                "text": "There are a number of different unmarked resistors available",
                "bbox": [
                    66,
                    223,
                    353,
                    238
                ],
                "page_number": 2,
                "is_pre": false,
                "issues": [],
                "is_pot_pre": false
            },
            {
                "type": "text",
                "text": "It is suggested that the relationship between \\(t\\) and \\(R\\) is",
                "bbox": [
                    66,
                    248,
                    322,
                    263
                ],
                "page_number": 2,
                "is_pre": false,
                "issues": [],
                "is_pot_pre": false
